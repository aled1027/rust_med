{
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:app:default",
    "core:event:default",
    "core:window:default",
    "core:webview:default",
    "core:path:default",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-create",
    "fs:allow-exists",
    "fs:scope-applocaldata-recursive",
    "fs:scope-temp-recursive",
    "shell:default",
    "shell:allow-execute",
    "shell:allow-spawn"
  ]
}